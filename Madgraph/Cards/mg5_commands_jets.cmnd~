import model MSSM_SLHA2-full
define x1 = x1+ x1-
define x2 = x2+ x2-
define p = p b b~
define j = j b b~
generate p p > x1 n2 @ 0
add process p p > x1 n2 j @ 1
add process p p > x1 n2 j j @ 2
output Generate/-89.34132400000014_101.5421130717493_49.36326758321987_-280.64663299999995/ppTOx1n2_jets
launch
shower=pythia8
detector=delphes
Cards/-89.34132400000014_101.5421130717493_49.36326758321987_-280.64663299999995/ppTOx1n2_jets
Cards/run_card.dat
Cards/pythia8_card.dat


